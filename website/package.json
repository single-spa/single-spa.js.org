{
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "eslint:check": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings 1",
    "eslint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --config .prettierrc --write '**/*.{ts,js,tsx,jsx,scss,css,html,md}'",
    "format:check": "prettier --config .prettierrc --check '**/*.{ts,js,tsx,jsx,scss,css,html,md}'"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.30",
    "@docusaurus/preset-classic": "^2.0.0-alpha.30",
    "classnames": "^2.2.6",
    "markdown-to-jsx": "^6.10.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
